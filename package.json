{
  "name": "@tinystacks/cdk-constructs",
  "version": "0.0.0",
  "description": "Modularized cdk constructs for use in building tailored stacks.",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "cleanup": "rm -rf dist || true && rm *.tgz || true",
    "clean-build": "npm ci && npm run cleanup && npm run build",
    "dependency-check": "./node_modules/.bin/depcheck",
    "lint": "./node_modules/.bin/eslint --ext .ts,.js ./",
    "lint-fix": "./node_modules/.bin/eslint --ext .ts,.js --fix ./",
    "lint-no-warn": "./node_modules/.bin/eslint --ext .ts,.js --quiet ./",
    "install-remote-packages": "echo \"no private packages to install\"",
    "major": "npm version major --no-git-tag-version",
    "minor": "npm version minor --no-git-tag-version",
    "patch": "npm version patch --no-git-tag-version",
    "package": "npm pack && cp tinystacks-byo-iac-templates-*.tgz tinystacks-byo-iac-templates-latest.tgz",
    "prepack": "npm run qa",
    "prerelease": "npm version prerelease --preid=local --no-git-tag-version",
    "publish-local": "npm run prerelease; bash ./localNpmLogin.sh; npm publish --@tinystacks:registry=http://local-npm-registry:4873",
    "qa": "npm run lint",
    "test": "QUIET=true jest --detectOpenHandles",
    "test-cov": "jest --coverage",
    "view-test-cov": "jest --coverage && open ./coverage/lcov-report/index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tinystacks/tinystacks-cdk-constructs.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tinystacks/tinystacks-cdk-constructs/issues"
  },
  "homepage": "https://github.com/tinystacks/tinystacks-cdk-constructs#readme",
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "@types/node": "^18.7.2",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "aws-cdk": "^2.37.1",
    "depcheck": "^1.4.3",
    "eslint": "^8.21.0",
    "eslint-import-resolver-typescript": "^3.4.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^28.1.3",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.37.1",
    "constructs": "^10.1.73",
    "prando": "^6.0.1",
    "source-map-support": "^0.5.21"
  }
}
